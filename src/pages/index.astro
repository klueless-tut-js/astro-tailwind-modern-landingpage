---
import PageLayout from '../layouts/PageLayout.astro';
import Hero from '../components/Hero.astro';
import Button from '../components/Button.astro';
import Qualities from '../components/Qualities.astro';
import Partners from '../components/Partners.astro'
import CTA from '../components/CTA.astro'
import ContactForm from '../components/ContactForm.astro'
---

<PageLayout title='HomeSmart'>
  <Hero />
  <main class="grid gap-12 sm:gap-16 md:gap-24 lg:gap-32 px-8 overflow-hidden">
    <Button variant='download' classExtra='mx-auto my-12'>Download the App</Button>
    <Qualities />
    <Partners />
    <CTA />
  </main>
  <ContactForm />
</PageLayout>